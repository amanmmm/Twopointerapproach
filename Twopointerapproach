#include <iostream>
using namespace std;
int main()
{
    int arr[8] = {1,2,3,4,5,6,7,8}; 
    int size = 8;
   int s = 0;
   int e = size-1;
   while (s<=e){
       if(s==e){
           cout << arr[s] << " ";
           break;
       }
       cout << arr[s] << " ";
       cout << arr[e] << " ";
       
       s++;
       e--;
   }
}
